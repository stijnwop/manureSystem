            </connector>
        </connectors>

        <valves>
            <valve type="8inch_brass" node="1">
                <handle type="handle_new" node="3">
                    <!-- Nodes below are child nodes of the handle node -->
                    <animation name="8inch_brass_handle_new">
                        <part node="0" startTime="0" endTime="0.25" startTrans="0 0 0" endTrans="0 0.10 0"/>
