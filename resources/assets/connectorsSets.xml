                </handle>
                <handle type="hydraulic" node="5" linkOffset="0.018 0.105 0"/>
                <handle type="pneumatic" node="6" linkOffset="0.018 0.105 0"/>
            </valve>
            <valve type="10inch_mz_castIron" node="7">
