                </handle>
                <handle type="handle_old" node="4">
                    <!-- Nodes below are child nodes of the handle node -->
                    <animation name="8inch_brass_handle_old">
                        <part node="0" startTime="0" endTime="0.5" startTrans="-0.070 0.240 0" endTrans="-0.070 0.395 0"/>
                        <part node="1" startTime="0" endTime="0.5" startRot="0 0 0" endRot="-25 0 0"/>
                        <part node="1|0" startTime="0" endTime="0.5" startRot="0 0 0" endRot="72.5 0 0"/>
                    </animation>
                </handle>
                <handle type="hydraulic" node="5"/><!-- Hydraulic -->
                <handle type="pneumatic" node="6"/><!-- Pneumatic -->
            </valve>
            <valve type="8inch_castIron" node="2">
