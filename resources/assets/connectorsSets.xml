<assets>
    <set filename="resources/assets/set_1.i3d">
        <connectors>
            <connector type="connector_1" node="0">
                <animation name="connector_1_lock">
                    <!-- Nodes below are child nodes of the connector node -->
                    <part node="0" startTime="0" endTime="0.5" startRot="0 0 -150" endRot="0 0 -15"/>
                    <part node="1" startTime="0" endTime="0.3" startRot="0 0 -20" endRot="0 0 -20"/>
