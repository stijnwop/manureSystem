                        <part node="1|0" startTime="0" endTime="0.25" startRot="0 0 0" endRot="0 0 50"/>
                        <part node="1|0" startTime="0.25" endTime="0.5" startRot="0 0 50" endRot="0 0 124"/>
                    </animation>
                </handle>
                <handle type="handle_old" node="4" linkOffset="0.018 0.145 0">
                    <!-- Nodes below are child nodes of the handle node -->
                    <animation name="8inch_castIron_handle_old">
                        <part node="0" startTime="0" endTime="0.5" startTrans="-0.070 0.240 0" endTrans="-0.070 0.395 0"/>
                        <part node="1" startTime="0" endTime="0.5" startRot="0 0 0" endRot="-25 0 0"/>
